{
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "no-class-assign": "off",
    "jsx-a11y/label-has-for": "off",
    "no-shadow": "off",
    "jsx-a11y/label-has-associated-control": "off",
    "arrow-body-style": "off"
  },
  "globals": {
    "document": true
  },
  "env": {
    "browser": true
  }
}
